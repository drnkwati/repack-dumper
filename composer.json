{
    "name": "drnkwati/repack-dumper",
    "description": "Mechanism for exploring and dumping PHP variables.",
    "keywords": ["dump", "debug"],
    "license": "MIT",
    "homepage": "https://github.com/drnkwati/repack-dumper",
    "support": {
        "source": "https://github.com/drnkwati"
    },
    "authors": [
        {
            "name": "Nkwati Divine",
            "email": "drnkwati@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.9"
    },
    "require-dev": {
        "symfony/var-dumper": ">=2.6"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Repack\\": "src"
        }
    },
    "autoload-dev": {
        "files": [
        ],
        "psr-4": {
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
